?start: iff

?iff: implication
    | iff "<->" implication -> iff

?implication: or
    | implication "->" or -> implication

?or: and
    | or "|" and -> or

?and: negation
    | and "&" negation -> and

?negation: "~" expression -> neg
    | expression

?expression: "(" iff ")"
    | variable -> var

?variable: ("a".."z")+

%import common.WS_INLINE
%ignore WS_INLINE